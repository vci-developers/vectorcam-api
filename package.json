{
  "name": "vectorcam-api",
  "version": "1.0.0",
  "description": "Node.js backend using Fastify",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/server.ts",
    "dev:test": "NODE_ENV=test ts-node-dev --respawn src/server.ts",
    "test": "NODE_ENV=test jest",
    "lint": "eslint src/**/*.ts",
    "start:dev": "NODE_ENV=development node dist/server.js",
    "start:test": "NODE_ENV=test node dist/server.js",
    "db:init": "ts-node scripts/init-db.ts",
    "db:init:force": "ts-node scripts/init-db.ts --force",
    "db:init:alter": "ts-node scripts/init-db.ts --alter",
    "db:seed:specimens": "ts-node scripts/seed-specimens.ts",
    "ci": "npm ci"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@aws-sdk/lib-storage": "^3.802.0",
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.35.0",
    "@sentry/profiling-node": "^9.35.0",
    "@sinclair/typebox": "^0.34.41",
    "@tus/s3-store": "^2.0.0",
    "@tus/server": "^2.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "pino": "^8.16.2",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250620.0",
    "@types/mysql": "^2.15.24",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "bun": "^1.2.17",
    "bun-types": "^1.2.17",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "pino-pretty": "^10.2.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
